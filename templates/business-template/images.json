      {
        "src": "./templates/business-template/gallery2.svg",
        "alt": "Team collaboration",
        "fallback": "./assets/placeholder-gallery.svg"
      },
      {
        "src": "./templates/business-template/gallery3.svg",
        "alt": "Office workspace",
        "fallback": "./assets/placeholder-gallery.svg"
