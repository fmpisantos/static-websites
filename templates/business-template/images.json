{
  "hero": {
    "type": "single",
    "src": "./templates/business-template/hero-image.svg",
    "alt": "Business professional hero image",
    "fallback": "./assets/placeholder-hero.svg"
  },
  "about": {
    "type": "single",
    "src": "./templates/business-template/about-image.svg",
